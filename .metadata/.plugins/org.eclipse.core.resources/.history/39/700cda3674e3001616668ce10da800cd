package p1;

import java.util.Arrays;

public class Demo {

	public static void main(String[] args) {
		BankAccount ba1 = new BankAccount(1, 100);
		BankAccount ba2 = new BankAccount(2, 200);
		BankAccount[] array = new BankAccount[3];
		array[0] = ba1;
		array[1] = ba2;
		Arrays.sort(array);
		System.out.println(Arrays.toString(array));
	}

}
